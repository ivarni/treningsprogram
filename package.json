  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 3000 --history-api-fallback --hot --config ./webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack.config.js",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint --fix src/ test/",
    "precommit": "npm run lint && npm run test",
    "heroku-postbuild": "npm run build",
    "test": "cross-env NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha -G --compilers js:babel-core/register --require test/setup.js --recursive test",
    "test:watch": "cross-env NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha -G --watch --compilers js:babel-core/register --require test/setup.js --recursive test",
    "posttest": "nyc report --reporter=html",
    "server": "cross-env NODE_ENV=production npm run build && node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivarni/treningsprogram.git"
